@misc{Hoagland2020,
  author = {Hoagland, Sarah Lucia},
  title = {Aspects of the Coloniality of Knowledge},
  note = {
  CORE ARGUMENT: Hoagland examines dimensions of the "coloniality of knowledge" by analyzing how Western epistemological frameworks structure what counts as legitimate knowledge and who counts as a knower. Drawing on postcolonial theory and decolonial scholarship, she identifies how colonial power relations persist in contemporary knowledge production practices, often invisibly reproducing hierarchies between global North and South, Western and non-Western knowledge systems.

  RELEVANCE: This paper provides conceptual clarification of "coloniality of knowledge," a central concept in Dussel's epistemology. Hoagland's analysis helps specify what exactly is "colonial" about dominant epistemological frameworks and how this differs from overt imperialism or cultural imperialism. The paper contributes to understanding the mechanisms by which Eurocentric epistemology maintains its dominance even after formal decolonization.

  POSITION: Decolonial epistemology. Analyzes structural features of colonial knowledge production and their persistence.
  },
  keywords = {coloniality-of-knowledge, epistemology, postcolonial-theory, Medium},
  abstract = {Looking at work on advocacy research, this article raises concerns about researchers, exploring and illustrating four aspects of the Coloniality of Anglo-European knowledge practice possible in such research. It suggests that it is not because we are able to be scholars that we are positioned to develop knowledge of marginalized others; it is because of how we are positioned in relation to marginalized others that we are able to be scholars. This article ends with a suggestion for an epistemic shift},
  abstract_source = {core},
}

@book{Barber2020,
  author = {Barber, Michael D.},
  title = {Ethical Hermeneutics: Rationality in Enrique Dussel's Philosophy of Liberation},
  publisher = {Fordham University Press},
  address = {New York},
  year = {2020},
  note = {
  CORE ARGUMENT: Barber presents Dussel's thought through the concept of "ethical hermeneutics," interpreting reality from the viewpoint of the Levinasian "Other"—those excluded from socio-political or cultural systems. He traces Dussel's development toward Levinas through engagement with Hegelian dialectics and stages of Dussel's ethical theory. Barber argues that Dussel's hermeneutics offers a rationality that is neither abstractly universal nor merely relative, but situated in the perspective of victims of domination.

  RELEVANCE: This is the most comprehensive English-language treatment of Dussel's epistemological-hermeneutic method. Barber's concept of "ethical hermeneutics" clarifies how Dussel's epistemology integrates ethical and interpretive dimensions: knowledge is always knowledge-for-someone, and the epistemic privilege of the oppressed derives from their material-ethical position. The book is essential for understanding the relationship between Dussel's epistemology and his broader philosophical project.

  POSITION: Sympathetic systematic exposition. Presents Dussel's epistemology as coherent alternative to both foundationalism and relativism through Levinasian ethics.
  },
  keywords = {ethical-hermeneutics, Levinas, philosophy-of-liberation, monograph, High, INCOMPLETE, no-abstract}
}

@article{Vizcaino2021,
  author = {Vizcaíno, Rafael},
  title = {Liberation Philosophy, Anti-Fetishism, and Decolonization},
  journal = {APA Newsletter on Hispanic/Latino Issues in Philosophy},
  year = {2021},
  volume = {20},
  number = {2},
  pages = {6--11},
  note = {
  CORE ARGUMENT: Vizcaíno offers a genealogy of the trope of fetishization in Dussel's liberation philosophy and argues that the "anti-fetishist" method central to this tradition risks perpetuating Eurocentrism. Drawing on recent cultural anthropology showing how the notion of "fetishism" emerges from Eurocentric anthropology of religion and misrepresents Afro-Atlantic religions, Vizcaíno contends that without revising the metaphysical premises of anti-fetishism, liberation philosophy contradicts its own commitment to epistemic decolonization.

  RELEVANCE: This is a provocative internal critique of Dussel's epistemological method. Vizcaíno argues that one of Dussel's key epistemological-critical concepts (anti-fetishism, derived from Marx) itself carries unexamined Eurocentric baggage. This challenges whether Dussel has successfully escaped Eurocentrism or has smuggled it in through his theoretical apparatus. The critique is important for assessing the coherence and consistency of Dussel's decolonial epistemology.

  POSITION: Critical internal assessment. Questions whether Dussel's anti-fetishism reproduces the very Eurocentrism it aims to overcome.
  },
  keywords = {anti-fetishism, internal-critique, decolonization, epistemology, Medium, INCOMPLETE, no-abstract}
}

@book{AlcoffMendieta2000,
  author = {Alcoff, Linda Mart{\'i}n and Mendieta, Eduardo},
  title = {Thinking from the Underside of History: Enrique Dussel's Philosophy of Liberation},
  publisher = {Rowman \& Littlefield Publishers},
  address = {Lanham, MD},
  year = {2000},
  note = {
