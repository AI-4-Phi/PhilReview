}

@article{gadamer1960truth,
  author = {Gadamer, Hans-Georg},
  title = {Truth and Method},
  year = {1960},
  note = {
  CORE ARGUMENT: Develops philosophical hermeneutics emphasizing that understanding always occurs within tradition through prejudices (pre-judgments). Argues Enlightenment's blanket rejection of prejudice was itself a prejudice—legitimate prejudices enable understanding by providing the horizon from which we interpret. Authority and tradition are not inherently opposed to reason but can be sources of genuine insight. However, prejudices must remain open to revision through dialogue and "fusion of horizons."

  RELEVANCE: Provides alternative framework to MacIntyre for understanding tradition's role in rationality, emphasizing how prejudices can be both enabling and limiting. Relevant to tradition breakdown: when prejudices ossify into dogma that resists dialogical revision, tradition loses its productive character. The concept of "fusion of horizons" suggests traditions fail when they can no longer engage in genuine dialogue with novel perspectives or challenges.

  POSITION: Gadamerian hermeneutics—tradition and prejudice are conditions of understanding, but must remain open to dialogical revision to avoid ossification.
  },
  keywords = {hermeneutics, Gadamer, prejudice, tradition, High}
}

@article{milic2024gadamer,
  author = {Milić, Aleksandar},
  title = {Gadamer's Rehabilitation of Prejudice, Authority and Tradition},
  journal = {Godišnjak Međunarodne filozofske škole Feliks Romulijana},
  year = {2024},
  doi = {10.46793/felixr24.16am},
  note = {
  CORE ARGUMENT: Explicates Gadamer's rehabilitation of prejudice, authority, and tradition as legitimate sources of knowledge, not obstacles to truth. Prejudices structure our understanding but are not fixed—they can be tested and revised through hermeneutic experience. Authority rests not on domination but on acknowledgment of superior knowledge. Tradition is the medium of understanding, providing the pre-understandings necessary for interpretation.

  RELEVANCE: Clarifies how prejudice and authority function productively in traditions while identifying conditions for their pathology. When authority becomes mere domination (rather than acknowledged expertise) or when prejudices become immune to revision, tradition degenerates. This provides criteria for distinguishing healthy tradition (open prejudices, legitimate authority) from pathological tradition (dogmatic prejudices, dominating authority).

  POSITION: Explicates Gadamerian view that prejudice and authority are properly understood as enabling rather than obstructing knowledge, when functioning healthily.
  },
  keywords = {Gadamer, prejudice, authority, tradition, hermeneutics, Medium}
}

@article{atkinson2014gadamer,
  author = {Atkinson, Camille Elise},
  title = {Gadamer, Tradition and Subjectivity},
  year = {2014},
  note = {
  CORE ARGUMENT: Examines criticisms of Gadamer's account of tradition from two opposing directions: conservatives charge him with undermining truth through relativistic emphasis on interpreter's prejudices, while progressives accuse him of validating oppressive power structures by insufficiently criticizing tradition. Argues Gadamer's position actually navigates between these extremes, showing how tradition enables critical reflection rather than simply constraining it.

  RELEVANCE: Addresses the tension between tradition as enabling and tradition as constraining—directly relevant to understanding tradition pathology. The dual critique reveals how traditions can fail in opposite directions: either ossifying into dogma that resists critical reflection or dissolving into subjectivism that abandons claim to truth. Successful traditions maintain productive tension between inheritance and critique.

  POSITION: Defends Gadamer's middle position—tradition both enables and constrains, requiring ongoing dialogue between inheritance and critical reflection.
  },
  keywords = {Gadamer, tradition-critique, conservatism, subjectivity, Medium}
}

