@article{Lozano2021,
  author = {Lozano Su{\'a}rez, Luz Mar{\'i}a},
  title = {Transmodernity: Method for a Decolonial Political Project. From Enrique Dussel to Santiago Castro-G{\'o}mez},
  journal = {Eidos},
  year = {2021},
  number = {34},
  pages = {199--225},
  doi = {10.14482/EIDOS.34.199.8},
  note = {
  CORE ARGUMENT: Lozano analyzes how Santiago Castro-Gomez appropriates and transforms Dussel's concept of transmodernity to propose an emancipatory politics. While Dussel's transmodernity (1994) aims to overcome modernity as a sacrificial, Eurocentric myth through affirmation of universal cultural otherness (analectic method), Castro-Gomez adapts this into "transmodern republicanism." Lozano examines the modernity/coloniality dialogue between these two philosophers to show how transmodernity functions as a method for decolonial politics, taking modernity's rational character but opposing domination.

  RELEVANCE: This article illuminates the internal decolonial debate between Dussel's liberation philosophy and Castro-Gomez's critical adaptation of it. The debate centers on whether Dussel's populist politics of liberation adequately addresses power complexity or whether Castro-Gomez's republicanism offers a better framework. Understanding this disagreement is essential for grasping how Dussel's concepts have been critically appropriated and transformed within decolonial thought.

  POSITION: Represents internal decolonial debate perspective. Lozano examines how Castro-Gomez builds on yet departs from Dussel, particularly regarding populism, republicanism, and the structure of emancipatory politics. The article shows productive disagreement within the modernity/coloniality/decoloniality research program.
  },
  keywords = {Dussel, Castro-Gomez, transmodernity, decolonial-politics, republicanism, Medium},
  abstract = {This article seeks to analyze how the sense of transmodernity is subsumed in phi- losopher Santiago Castro-Gómez, to propose an emancipatory policy. Transmodernity is a decolonial project proposed by Enrique Dussel in 1994. Modernity, defined as a sacrificial myth -eurocentric, colonizing, and capitalist- would be overcome from the moment in which it is possible to affirm the otherness of the universal cultures —ana- lectic—. Such a project of the Philosophy of Liberation can be conceived as a method that would take the best of modernity —its rational character—, but in an opposite sense to political, cultural, and economic domination. Gastro-Gómez assumes this method to propose a transmodern republicanism. In this text, the possible modernity/ coloniality dialog, corroborated by these two philosophers, will be established as the guiding thread to determine how a new way of understanding the ontology of eman- cipatory (not liberating) politics, presented by the Colombian philosopher, is possible.},
  abstract_source = {s2},
}

