@article{Irvine2011,
  author = {Irvine, Andrew B.},
  title = {An Ontological Critique of the Trans-Ontology of Enrique Dussel},
  journal = {Sophia},
  year = {2011},
  volume = {50},
  number = {4},
  pages = {603--624},
  doi = {10.1007/s11841-010-0210-8},
  note = {
  CORE ARGUMENT: Irvine offers an ontological critique of Dussel's "trans-ontology" and analectic method. He questions whether Dussel's analectic approach genuinely goes beyond Western thinking about being, as Dussel claims. Irvine proposes an ontological critique that is "classically Western" yet "classically relevant" even in the context of globalization and exclusion. He argues that Dussel's attempt to move beyond ontology to ethics of the Other does not escape the conceptual frameworks of Western metaphysics.

  RELEVANCE: This is a rare analytic philosopher's engagement with Dussel's core methodological claims about trans-ontology and analectics. Irvine's critique challenges whether Dussel's philosophical innovation (moving beyond ontology through the ethical relation to the Other) succeeds or whether it remains captive to the Western philosophical tradition it seeks to transcend. The critique raises fundamental questions about the coherence of Dussel's philosophical method.

  POSITION: Represents an analytic philosopher's skepticism about continental and Latin American philosophy's attempts to transcend Western ontology. Irvine argues for the continuing relevance of "historically Western" yet "classically relevant" ontological frameworks, implicitly questioning the necessity and success of Dussel's decolonial philosophical project.
  },
  keywords = {Dussel, trans-ontology, analectics, ontological-critique, Western-philosophy, High, INCOMPLETE, no-abstract}
}

@article{DiazFernandez2022,
