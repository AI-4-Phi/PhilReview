@article{Ghosh2019dependency,
  author = {Ghosh, B. N.},
  title = {Dependency Theory Revisited},
  year = {2019},
  publisher = {Routledge},
  doi = {10.4324/9781315187389},
  note = {
  CORE ARGUMENT: Ghosh provides a comprehensive survey of dependency theory
  from its classical formulations (Prebisch-Singer, Frank, Amin) through
  contemporary revisions. The book examines peripheral writers on dependency
  including Cardoso, Sunkel, Wallerstein, and Myrdal, situating dependency
  theory within broader debates about development, underdevelopment, and unequal
  exchange. Contemporary sections address globalization, technological
  imperialism, and brain drain as new forms of dependency.

  RELEVANCE: While not focused on Dussel specifically, this comprehensive
  overview situates dependency theory—a key intellectual source for Dussel's
  philosophy of liberation—within global development debates. Understanding
  dependency theory's evolution helps clarify how Dussel's Eurocentrism critique
  emerged from Latin American political economy rather than solely from European
  philosophy. The book's treatment of Frank, Wallerstein, and Amin shows the
  intellectual milieu that shaped Dussel's work.

  POSITION: Provides intellectual-historical context for understanding dependency
  theory's influence on Dussel's thought.
  },
  keywords = {dependency-theory, world-systems, intellectual-history, Low}
}

@misc{MaldonadoTorres2006thinking,
  author = {Maldonado-Torres, Nelson},
  title = {Thinking From the Limits of Being: Levinas, Fanon, Dussel and the Cry of Ethical Revolt},
  year = {2006},
  howpublished = {PhD dissertation, University of California, Berkeley},
  note = {
  CORE ARGUMENT: Maldonado-Torres examines three thinkers who rethink the human
  in post-imperial ways: Levinas, Fanon, and Dussel. He analyzes how each
  responds to European master morality of domination and control. The dissertation
  evaluates Dussel's appropriation of Levinas through analysis of Fanon, arguing
  that while Dussel should have been more Fanonian than Levinasian at certain
  points, his work contributes importantly to overcoming Western master morality
  of aggression and control.

  RELEVANCE: This dissertation is crucial for understanding Dussel's relationship
  to both European philosophy (Levinas) and African/Caribbean decolonial thought
  (Fanon). Maldonado-Torres suggests that Dussel's Levinas engagement may limit
  his decolonial radicality compared to Fanon's more thoroughgoing critique. This
  comparative framework helps assess whether Dussel's European philosophical
  sources constrain his Eurocentrism critique.

  POSITION: Critical comparative position—suggests Fanon provides more radical
  decolonial resources than Levinas, questioning Dussel's philosophical choices.
  },
  keywords = {Dussel-Levinas-Fanon, comparative, decolonial, Medium}
}

@article{Davis2018globalization,
  author = {Davis, Benjamin P.},
  title = {Globalization/Coloniality: A Decolonial Definition and Diagnosis},
  journal = {Transmodernity: Journal of Peripheral Cultural Production of the Luso-Hispanic World},
  year = {2018},
  volume = {8},
  pages = {},
  doi = {10.5070/T484042045},
  note = {
  CORE ARGUMENT: Davis argues that globalization is better understood as a
  colonial project than an international process. Drawing on Dussel's
  "transmodernity," Quijano's "global coloniality," and Mendieta's "dialogical
  cosmopolitanism," Davis develops a decolonial definition of globalization that
  foregrounds its violences and colonial continuities. He proposes "affective
  alternatives" as ways of life different from globalization's hegemonic forms,
  creating a parallax that exposes globalization as globalization/coloniality.

  RELEVANCE: This article shows how contemporary decolonial scholars use Dussel's
  concepts (particularly transmodernity) to analyze present-day global power
  structures. It situates Dussel alongside Quijano and Mendieta in developing
  analytical tools for understanding coloniality's persistence. The article
  demonstrates Dussel's influence on current decolonial scholarship and his role
  in connecting historical colonialism to contemporary globalization.

  POSITION: Positions Dussel as foundational for contemporary decolonial analysis
  of globalization.
  },
  keywords = {transmodernity, globalization, coloniality, Medium}
}

@article{Misoczky2019contributions,
  author = {Misoczky, Maria Ceci},
  title = {Contributions of An{\'i}bal Quijano and Enrique Dussel for an Anti-Management Perspective in Defence of Life},
  journal = {Cuadernos de Administraci{\'o}n},
  year = {2019},
  doi = {10.11144/javeriana.cao.32-58.caqed},
  note = {
  CORE ARGUMENT: Misoczky examines how Quijano's "coloniality of power" and
  Dussel's "transmodernity" provide resources for anti-management perspectives
  that defend life against capitalist domination. She affirms the Philosophy of
  Liberation as an ethical foundation for Latin American studies and argues for
  an "anti-management attitude" that transcends North/South management
  distinctions. The article critically engages postcolonial studies and argues
  for the superiority of decolonial approaches (Quijano, Dussel) for analyzing
  capitalist power's victims.

  RELEVANCE: This article demonstrates how Dussel's work (particularly in
  dialogue with Quijano) informs contemporary critical management and
  organizational studies. It shows Dussel's influence extending beyond philosophy
  into social science applications. The comparison between Dussel and Quijano
  illuminates Dussel's distinctive contributions to decolonial theory—particularly
  his ethical-philosophical framework compared to Quijano's sociological analysis.

  POSITION: Positions Dussel and Quijano as complementary decolonial theorists
  providing ethical and sociological resources for anti-capitalist critique.
  },
  keywords = {Dussel-Quijano, coloniality-of-power, transmodernity, Low}
}
