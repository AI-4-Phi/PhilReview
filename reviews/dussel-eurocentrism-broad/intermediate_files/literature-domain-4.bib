  abstract_source = {s2},
}

@incollection{Saenz2021,
  author = {Sáenz Rovner, Mario},
  title = {Ideality and Intersubjectivity: Dialectics and Analectics in a Philosophy of Liberation},
  booktitle = {Decolonizing Ethics: The Critical Theory of Enrique Dussel},
  editor = {Allen, Amy and Mendieta, Eduardo},
  year = {2021},
  publisher = {Pennsylvania State University Press},
  pages = {89--112},
  note = {
  CORE ARGUMENT: Examines Dussel's method of "analectics" as alternative to Hegelian dialectics. Argues that while dialectics proceeds through internal negation and sublation, analectics begins from the revelation of the Other who stands outside the totality. This methodological difference grounds Dussel's political philosophy: politics must begin from the exteriority of the excluded rather than from immanent contradictions of existing institutions.

  RELEVANCE: Essential for understanding the philosophical method underlying Dussel's political philosophy. The analeptic method—beginning from the Other's revelation rather than systemic contradiction—provides the basis for Dussel's critique of Eurocentric political theory that starts from constituted political orders. Relevant for assessing whether Dussel's method successfully escapes Hegelian-Marxist frameworks or remains dialectical.

  POSITION: Sympathetic reconstruction of Dussel's method; treats analectics as viable alternative to dialectics while noting philosophical challenges in grounding politics in revelation of exteriority.
  },
  keywords = {analectics, method, intersubjectivity, Medium, INCOMPLETE, no-abstract}
}
