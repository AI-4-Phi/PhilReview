@article{Hoston2025,
  author = {Hoston, Germaine A.},
  title = {A Neo-Confucian "Theology of Liberation"? Humanism and Ethics in Levinas, Liberation Theology, and Wang Yangming},
  journal = {Harvard Theological Review},
  year = {2025},
  volume = {118},
  pages = {85--109},
  doi = {10.1017/S0017816025000069},
  language = {english},
  note = {
  CORE ARGUMENT: Hoston compares Dussel's liberation theology and ethics with Wang Yangming's Neo-Confucian philosophy, identifying parallel epistemological and ethical critiques of orthodoxy. Both Dussel and Wang critique institutionalized orthodoxy for excluding the ethical perspective of the marginalized, and both ground ethics in the primacy of lived experience over abstract doctrine. Hoston argues that Wang's Idealist philosophy contains democratic and liberationist elements that prefigure liberation theology.

  RELEVANCE: Demonstrates the cross-cultural relevance of Dussel's ethical critiques by comparing them to non-Western philosophical traditions. Shows how Dussel's critique of Eurocentric universalism parallels Wang's critique of Rationalist Neo-Confucian orthodoxy. The comparison illuminates what is distinctive about Dussel's approach: grounding ethics in the appeals of victims and emphasizing Levinasian alterity.

  POSITION: Comparative study identifying structural parallels between Dussel's liberation ethics and Neo-Confucian critiques of orthodoxy.
  },
  keywords = {comparative-ethics, Levinas, liberation-theology, Medium},
  abstract = {Abstract This article reconsiders the sixteenth-century Idealist Neo-Confucian philosophy of Wang Yangming (1472–1529) in light of the development of twentieth-century Latin American liberation theology. After defining liberation theology, this study identifies the crucial contributions made to it by Emmanuel Levinas’s assertion of the primacy of ethics over ontology and critique of the egocentric nature of Western philosophy. It then delineates the epistemological and deontological criticisms made of Roman Catholic orthodoxy—and institutionalized Christianity in general—by Latin American liberation theologians, particularly Enrique Dussel and José Porfirio Miranda. These are compared with Wang’s critique of the Rationalist Neo-Confucianism that had been official orthodoxy and the legitimating philosophy for imperial China for three centuries. The study finds that Wang’s Idealist philosophy incorporates epistemological, spiritual, and ethical perspectives with powerful democratic and liberationist elements that prefigure the development of late-twentieth-century Latin American liberation theology. Thus, contrary to the conventional view of Confucianism as a conservative philosophy, these elements in Wang’s Neo-Confucianism render it a theology (or philosophy) of liberation.},
  abstract_source = {s2},
}

@article{Demenchonok2012,
  author = {Demenchonok, Edward},
  title = {Rethinking Kant's Concept of Human Rights as Freedom},
  journal = {Filosofia Unisinos},
  year = {2012},
  volume = {13},
  number = {Suppl.},
  pages = {133--150},
  doi = {10.4013/fsu.2012.132(suppl).09},
  language = {english},
  note = {
  CORE ARGUMENT: Demenchonok examines debates over grounding human rights in a pluralistic world, analyzing Kantian, discourse-ethical (Apel, Habermas), and liberation-ethical (Dussel) approaches. He argues that while discourse ethics develops Kant's proceduralism, Dussel's ethics of liberation grounds human rights in material dignity and the concrete needs of victims. Demenchonok defends the continuing relevance of Kantian cosmopolitanism while acknowledging Dussel's critique of abstract universalism.

  RELEVANCE: Situates Dussel's ethics within broader debates over human rights and universalism. Shows how Dussel's material principle responds to the problem of grounding rights in culturally diverse contexts—not through abstract reason but through the universality of corporeal vulnerability. Clarifies how Dussel's critique of Kant differs from postmodern relativism.

  POSITION: Comparative analysis arguing for complementarity between Kantian cosmopolitanism and Dussel's material ethics.
  },
  keywords = {human-rights, Kant, discourse-ethics, material-ethics, Medium},
  abstract = {The paper examines the current debates regarding the grounding of human rights in a pluralistic, culturally diverse world. It analyses the challenges which come today from certain policies of human rights which instrumentalize them under the pretext of a “global war on terror” and redefi ne them in terms of democracy promotion and regime change, as well as those challenges which come from ideologies which question the core principles of human rights and provoke the so called “crisis of legitimization.” The paper argues for the necessity of restoring the genuine meaning of human rights and of the role of citizens in striving for their implementation. At the heart of the analysis is Kant’s concept of human rights as freedom. With Kant as its backdrop, the paper shows that the claim that religious worldviews provide the only intelligible foundation, as in Nicholas Wolterstorff, faces serious challenges in a pluralistic, culturally diverse world. The paper then analyzes the non-religious philosophical concepts which seek to develop an account of human rights that does not depend upon controversial metaphysical or religious doctrines. It examines an account of human rights from the perspective of discourse ethics as developed by Karl-Otto Apel and Jurgen Habermas. It also analyses the concepts of human dignity and “emerging morality” by Arturo Roig and the “ethics of liberation” by Enrique Dussel. The analysis shows the ongoing relevance of Kant’s ideas and their recent development by the theorists of discourse ethics and of dialogical and democratic cosmopolitanism. O texto examina os debates atuais a respeito da fundamentacao dos direitos humanos em um mundo pluralista e culturalmente diversifi cado. Analisa os desafi os que surgem atualmente de certas politicas de direitos humanos que os instrumentalizam sob o pretexto de uma “guerra global contra o terror” e os redefi ne em termos de promocao da democracia e regime de mudanca, assim como aqueles desafi os que surgem de ideologias que questionam os principios basicos dos direitos humanos e provocam a assim chamada “crise de legitimacao”. O texto argumenta pela necessidade de restaurar o signifi cado genuino de direitos humanos e do papel dos cidadaos em lutar por sua imple},
  abstract_source = {s2},
}

@article{DiazFernandez2022,
  author = {Díaz Fernández, José},
  title = {Desde las éticas hacia la política de la liberación de Enrique Dussel. Aproximación crítica y debates sobre su filosofía de la liberación},
  journal = {Revista de filosofía},
  year = {2022},
  volume = {79},
  pages = {61--82},
  doi = {10.4067/s0718-43602022000100061},
  language = {spanish},
  note = {
  CORE ARGUMENT: Presents a critical reading of Dussel's philosophy tracing continuities and ruptures between his early and mature ethical-political projects. Examines Dussel's early ethical and political positions, Horacio Cerutti's critique, the structure of mature ethics, the debate with Karl-Otto Apel, the structure of politics of liberation, and Santiago Castro-Gómez's critique. Offers a secular and critical reading of Dussel's liberation philosophy.

  RELEVANCE: Important Spanish-language critical assessment mapping Dussel's intellectual development and major debates. Essential for understanding internal critiques within Latin American philosophy (Cerutti, Castro-Gómez) and Dussel's engagement with European discourse ethics (Apel). Shows how Dussel's political philosophy emerged from and responded to debates within liberation philosophy movement.

  POSITION: Critical but engaged assessment; treats Dussel as central figure in Latin American philosophy while identifying tensions and unresolved problems in his system.
  },
  keywords = {politics-of-liberation, discourse-ethics, Latin-American-philosophy, High},
  abstract = {Este artículo presenta una lectura crítica de la filosofía de Enrique Dussel, donde se consideran los quiebres y continuidades de su proyecto ético temprano y maduro. El objetivo de la investigación es explorar y exponer el pensamiento del autor en relación con las problemáticas, críticas y polémicas que presenta. Para esto se esclarecen las posiciones éticas y políticas tempranas de su filosofía, la atingencia de la crítica de Horacio Cerutti, la estructura de la ética madura, el debate con Karl-Otto Apel, la estructura de la política de la liberación y la crítica de Santiago Castro-Gómez. El texto presenta una lectura secular y crítica de una de las filosofías de la liberación más relevantes en Latinoamérica.},
  abstract_source = {openalex},
}

@article{QuintanaPaz2014,
  author = {Quintana Paz, Miguel Ángel},
  title = {Enrique Dussel and Liberation Theology: Violence or Dialogue?},
  journal = {Philosophia},
  year = {2014},
  volume = {42},
  number = {1},
  pages = {71--92},
  language = {english},
  note = {
  CORE ARGUMENT: Quintana Paz argues that Dussel's ethics of liberation contains internal contradictions when applied to the question of revolutionary violence. While Dussel endorses three criteria (formal-dialogical, material-welfarist, feasibility-strategic) that any legitimate revolution must satisfy, Quintana Paz contends that each criterion is conceptually incompatible with violence. He concludes that Dussel's arguments involve contradictions when he aims to justify violence with these interdisciplinary criteria.

  RELEVANCE: Addresses a key tension in Dussel's ethics: can an ethics grounded in discourse and life-affirmation ever justify revolutionary violence? Quintana Paz argues that Dussel's formal principle (derived from discourse ethics) requires dialogue, which excludes violence. This critique raises questions about whether Dussel's ethics can support the political praxis it aims to ground.

  POSITION: Critical argument that Dussel's ethics is incompatible with justification of revolutionary violence.
  },
  keywords = {ethics-liberation, violence, liberation-theology, discourse-ethics, Medium, INCOMPLETE, no-abstract}
}

@article{Knauss2013,
  author = {Knauss, Stefan},
  title = {De la crítica a la normatividad? Materialidad y exterioridad como puntos de partida para una ética universal?},
  journal = {Ars Brevis},
  year = {2013},
  pages = {216--236},
  publisher = {Edicions de la Universitat de Barcelona},
  language = {spanish},
  note = {
  CORE ARGUMENT: Knauss analyzes Dussel's Ética de la liberación as a critical political-philosophical project that begins from the perspective of excluded persons in former colonies. He examines how Dussel uses the material criterion of life to demonstrate unjust distribution of political and economic goods, and how the denial of dignity to the poor functions as a negative value in comparative evaluation of political theory and practice. Knauss explores whether Dussel successfully moves from critique to normativity.

  RELEVANCE: German-Spanish philosophical engagement with Dussel's ethics, examining the relationship between critical diagnosis and normative prescription. Addresses the question of how Dussel grounds universal norms from the factual existence of excluded human beings. Shows how Dussel's concept of exteriority functions as both critical standpoint and normative foundation.

  POSITION: Analysis of how Dussel moves from material critique to normative ethics grounded in exteriority.
  },
  keywords = {material-ethics, exteriority, normativity, spanish, Medium},
  abstract = {El siguiente texto analiza el enfoque de Enrique Dussel en su obra La ética de la liberación, en tanto que proyecto filosófico político crítico. El punto de partida performativo de la filosofía de Dussel es una crítica al liberalismo occidental, y a su eurocentrismo inherente, desde la perspectiva de las personas excluidas en las antiguas colonias. Por tanto, Dussel utiliza el criterio material de la vida para mostrar la injusta distribución de los bienes políticos y económicos. La pretendida negación de la dignidad de los pobres funciona, entonces, como un valor negativa en la evaluación comparativa de la calidad de la teoría y la práctica políticas. Desde un fundamento fáctico, la misma existencia de seres humanos excluidos cuestiona la normatividad de teorías de la justicia. Normatividad que, por otra parte, para Dussel se puede empoderar gracias a las demandas de estos grupos críticos que anhelan su reconocimiento cultural, económico y político},
  abstract_source = {core},
}

@article{Foscolo2013,
  author = {Fóscolo, Norma},
  title = {A "biopolitics" for Latin America},
  journal = {Millcayac: Revista Digital de Ciencias Sociales},
