@misc{StoneMediatore2023,
  author = {Stone-Mediatore, Shari},
  title = {Global Ethics, Epistemic Colonialism, and Paths to More Democratic Knowledges},
  howpublished = {Philpapers},
  note = {
  CORE ARGUMENT: Stone-Mediatore analyzes how colonialist elements implicit in prominent global ethics anthologies foster attitudes of superiority and aloofness toward economically struggling communities, even when these texts advocate aid to "the global poor." Drawing on Dussel, Linda Tuhiwai Smith, and other scholars of colonialism, she traces colonialist legacies in global ethics literature and proposes strategies for unsettling this epistemic colonialism and cultivating skills for more democratic global knowledge communities.

  RELEVANCE: This paper provides a concrete case study of epistemic colonialism in contemporary academic practice (global ethics pedagogy and scholarship). Stone-Mediatore applies Dussel's insights about geopolitics of knowledge to show how seemingly neutral or even progressive ethical frameworks reproduce colonial power relations through knowledge production and circulation. The paper is crucial for demonstrating the practical implications of Dussel's epistemological critique beyond abstract philosophical debate.

  POSITION: Applied decolonial epistemology. Demonstrates how epistemic colonialism operates in global ethics discourse and proposes pedagogical interventions.
  },
  keywords = {epistemic-colonialism, global-ethics, applied-epistemology, pedagogy, High},
  abstract = {In recent decades, the literature of global ethics has promoted greater and more rigorous attention to transnational moral responsibilities. This essay argues, however, that prominent global-ethics anthologies remain burdened by Eurocentric/colonialist elements that contradict efforts to build more ethical transnational communities. Drawing on scholars of coloniality, including Enrique Dussel, Anibal Quijano, and Linda Tuhiwai Smith, the essay traces colonialist elements in deep structures of prominent global ethics texts. It examines how, even when texts argue for aid to the poor, these elements foster tendencies in the affluent world to detach from and dehumanize people on the other side of global hierarchies. They also deprive academic readers of the insights of grassroots global-justice struggles. The essay concludes by sketching some directions that those of us who study and teach global ethics might pursue in order to unsettle colonialist baggage and cultivate skills and relationships more conducive to ethical global communities},
  abstract_source = {core},
}

