@article{Padilla2023,
  author = {Padilla, Javier},
  title = {Political Theology from the Global South: Enrique Dussel and the Poetics of Liberation},
  journal = {Interventions},
  year = {2023},
  volume = {26},
  number = {3-4},
  pages = {490--512},
  doi = {10.1080/1369801X.2023.2290573},
  note = {
  CORE ARGUMENT: Argues that Dussel's philosophy of liberation provides a decolonial corrective to Eurocentric political theology (Badiou, Žižek, Agamben) by centering early Christian thought on community and collectivity from the perspective of the Global South. Demonstrates this through a reading of Dussel's study of Marx's theological metaphors and applies the framework to Caribbean poet Pedro Mir's "Countersong to Walt Whitman," showing how Mir constructs a redemptive state of rebellion against imperial exception.

  RELEVANCE: Essential for understanding how Dussel's political philosophy engages with the political theology tradition while critiquing its Eurocentric foundations. Shows how Dussel's liberationist thought can be applied to literary and cultural analysis as a "poetics of liberation" that unravels the transformation of political and theological discourses. Directly addresses the research project's concern with Dussel's alternative to European political frameworks.

  POSITION: Sympathetic reconstruction of Dussel's political theology as corrective to European political theology; situates Dussel as precursor to contemporary debates on Christian Marxism from Global South perspective.
  },
  keywords = {political-theology, liberation-theology, transmodernity, High},
  abstract = {In recent decades, thinkers like Alain Badiou, Slavoj Žižek, and Giorgio Agamben have argued for a serious political engagement with early Christian thought and its emphasis on collectivity and community. Notwithstanding their important theoretical contributions, these writers often remain within a limited Eurocentric framework, foreclosing the possibility of a truly decolonial political theology. Crucially, their relatively recent reappraisals tend to ignore important precursors of Christian Marxism from the Global South, namely liberation theology and the philosophy of liberation, as elaborated by the Latin American philosopher Enrique Dussel. This essay contextualizes Dussel’s liberational thought in the contexts of political theology and literary representation. It pays particular attention to Dussel’s study of the Theological Metaphors of Marx by synthesizing a model of literary inquiry based on his philosophy and applying it to a reading of counter-hegemonic poetry by the Caribbean poet Pedro Mir, namely his long poem “Countersong to Walt Whitman.” This reading makes explicit the implicit theological dimension of Mir’s poetic text. Finally, the essay conceptualizes this interpretive model as a poetics of liberation: a mode of engagement with mutually determining discourses – politics, poetics, aesthetics – as well as a mode of reading which unravels the transformation of these discourses in lyrical forms. Writing from the periphery of the US empire, Mir therefore constructs a mode of address which arms itself with both political and theological discourses, however implicitly. To the empire’s miraculous state of exception, Mir counterpoises a redemptive state of rebellion.},
  abstract_source = {s2},
}

@article{Achenbach2021,
  author = {Achenbach, Alina},
  title = {Non-Europeans and their Presence to History. Universality, Modernity and Decolonization in Ricœur's Political Philosophy},
  journal = {Études Ricoeuriennes / Ricoeur Studies},
  year = {2021},
  volume = {12},
  number = {1},
  pages = {106--123},
  doi = {10.5195/ERRS.2021.535},
  note = {
  CORE ARGUMENT: Examines the exchange between Ricœur and his former student Dussel on decolonization and European responsibility. Analyzes Ricœur's concept of the "crisis of the concrete universal in Western Europe" and its relation to cultural difference, then critically discusses whether Ricœur's explicitly European perspective can sustain a viable world-historical subject in light of Dussel's critique of Eurocentric modernity.

  RELEVANCE: Important for assessing how Dussel's critique of Eurocentrism engages with and challenges European hermeneutic philosophy. The Ricœur-Dussel dialogue illuminates tensions between European universalism and decolonial pluriversality, particularly regarding who can be a legitimate subject of history. Shows how Dussel's challenge prompted European philosophers to confront their own positionality.

  POSITION: Critical assessment of both Ricœur and Dussel; questions whether European self-reflection can adequately respond to decolonial critique while noting limitations in Dussel's alternative conception of intercultural dialogue.
  },
  keywords = {universality, hermeneutics, decolonization, Medium},
  abstract = {This article reconsiders Paul Ricœur’s political philosophical writings on the task of decolonization and European responsibility in light of a horizon of intercultural dialogue. Departing from the exchange between Ricœur and his former student Enrique Dussel, it discusses the Ricœurian critique of modernity. After giving some background on Ricœur’s reflections on decolonization, it will clarify what Ricœur calls the “crisis of the concrete universal in the thinking and in the historical experience of Western Europe,” and what role cultural difference has in this crisis. Considering Ricœur’s explicitly European perspective, this will lead us to a critical discussion on the envisioned possibility of a world-historical subject. Finally, I will discuss the role of European self-reflection in relation to a horizon of intercultural dialogue and narration from a post-/decolonial perspective.},
  abstract_source = {s2},
}

@article{SagerSpencer2016,
