@article{Davis2018,
  author = {Davis, Benjamin P.},
  title = {Globalization/Coloniality: A Decolonial Definition and Diagnosis},
  journal = {TRANSMODERNITY: Journal of Peripheral Cultural Production of the Luso-Hispanic World},
  year = {2018},
  volume = {4},
  number = {8},
  doi = {10.5070/t484042045},
  note = {
  CORE ARGUMENT: Contends that globalization is less an international process than a colonial project. Develops a definition of globalization drawing on Dussel's transmodernity, Quijano's global coloniality, and Mendieta's dialogical cosmopolitanism to account for violences that economic metrics miss. Proposes "affective alternatives" to hegemonic social forms promoted by globalization/coloniality.

  RELEVANCE: Shows how Dussel's transmodernity concept is appropriated in contemporary analyses of globalization as a continuation of colonial modernity. The modernity/coloniality coupling is essential to understanding how Eurocentrism operates not just historically but in present global structures.

  POSITION: Constructive appropriation for contemporary diagnosis of globalization.
  },
  keywords = {globalization, transmodernity, coloniality, Medium},
  language = {english},
  abstract = {In this essay, I contend that globalization is less an international process and more a colonial project. I argue that definitions of “globalization” articulated primarily through economic metrics insufficiently account for the violences concomitant with such a project. In response to this insufficiency, I draw on three concepts by three decolonial authors—transmodernity (Enrique Dussel), global coloniality (Anibal Quijano), and dialogical cosmopolitanism (Eduardo Mendieta)—in order to develop my own definition of globalization. I then offer a preliminary sketch of what I call “affective alternatives,” which could convey ways of life different from the hegemonic social forms that globalization promotes and imposes. At the very least, affective alternatives present a parallax: seen and felt from the perspective of the alternative, globalization is not taken as neutral, normal, irreversible, or desired; rather, it is “distorted” into globalization/coloniality, a project to be resisted. I conclude with reflections on connecting and deepening coalitions of resistance.},
  abstract_source = {s2},
}

@article{Orrego2025,
  author = {Orrego Machuca, Pablo Arturo},
  title = {Walter Benjamin y el materialismo mesi\'{a}nico: di\'{a}logo desde las lecturas de Bol\'{i}var Echeverr\'{i}a y Enrique Dussel},
  journal = {Sin Fundamento},
  year = {2025},
  doi = {10.18041/1692-5726/sin_fundamento.32.2024.12515},
  note = {
  CORE ARGUMENT: Analyzes Walter Benjamin's critique of progress as central to capitalist and socialist modernity, drawing on interpretations by Echeverría and Dussel. Argues that Benjamin's vision of progress as historical construction hiding underlying barbarity in power structures connects with Latin American liberation philosophies' need for a new critical look at history centered on resistance and transformation of the oppressed.

  RELEVANCE: Connects Dussel's myth of modernity to Benjamin's "Theses on the Philosophy of History" and the critique of historicism's victors' perspective. Illuminates how Dussel's anti-Eurocentric historiography shares with Benjamin a concern to interrupt triumphalist narratives of progress.

  POSITION: Comparative analysis linking Dussel to Frankfurt School messianic materialism.
  },
  keywords = {benjamin, progress-critique, messianism, Medium},
  language = {spanish},
  abstract = {This article analyzes the work of Walter Benjamin, focusing on his "Theses on the Philosophy of History" and his critique of progress as a central axis of capitalist and socialist modernity. Drawing on the reflections of Bolívar Echeverría and Enrique Dussel, the text argues, from a reading that intertwines Benjamin's Jewish condition and his connection with European culture, that the vision of progress is a historical construction that hides the underlying barbarity in power structures, both in capitalism and in real socialism. When the author proposes his interpretation of history "against the grain," he challenges the linear and triumphalist conception of progress, proposing a historical materialism based on the memory of the oppressed and the redemption of the victims of the past. The work also highlights how Benjamin's critique of modernity, far from being an abstract reflection, has a practical and revolutionary dimension, linking his ideas with Latin American liberation philosophies and the need for a new critical look at history. In this sense, the article argues that theology, in Benjamin's thought, does not propose a doctrine, nor a treatise on God, but rather a critical tool that allows for the interruption of the hegemonic narrative of progress and the reconfiguration of social struggles from a radically new perspective, centered on resistance and the historical transformation of the oppressed.},
  abstract_source = {s2},
}

@misc{AyalaColqui2019,
  author = {Ayala-Colqui, Jes\'{u}s},
  title = {The Context of Modern Politics},
  year = {2019},
  howpublished = {PhilPapers preprint},
  note = {
  CORE ARGUMENT: Under the guiding thread of a de-struction of world political history from the position of suffering living corporality, presents Dussel's context of modern politics from Política de la liberación. Modernity is not a merely intra-European phenomenon but a moment arising from the experience of colonization of the non-European Other. 1492 is the moment of the "birth" of Modernity as concept, the origin of a myth of sacrificial violence and a process of "covering up" the non-European.

  RELEVANCE: Clarifies the exact nature of Dussel's 1492 periodization claim: modernity's origin is not the Renaissance or Reformation (intra-European events) but the colonial encounter that simultaneously produces Europe's self-conception as "modern" and enacts violence on colonized peoples. The "covering up" (encubrimiento) is central to the myth thesis.

  POSITION: Expository account of Dussel's mature political philosophy.
  },
  keywords = {1492-periodization, covering-up, political-philosophy, Medium},
  language = {english},
  abstract = {Under the guiding thread of a de-struction  of world political history, and from the position of suffering living corporality, Enrique Dussel presents the context of modern politics in the fifth section of Política de la liberación: Historia mundial y crítica. Modernity is not in this case a merely intra-European phenomenon that belongs to a periodization of history enunciated from the West. It is, on the contrary, a moment that arose from the experience of colonization of the non-European Other. As Dussel argues, 1492 will be the moment of the ‘birth’ of Modernity as a concept, the concrete moment of the ‘origin’ of a very particular ‘myth’ of sacrificial violence and, at the same time, a process of ‘covering up’ of the non-European. Understanding the context of the emergence of this colonizing experience, that is to say, of the other worlds of life before and outside of this European Modernity, is the objective of this fifth section. For this, four moments are addressed in the text: China, the Ottoman Empire, pre-modern and eastern Venice, the Italian Renaissance.Educatio},
  abstract_source = {core},
}

@misc{Deere2025book,
  author = {Deere, Don Thomas},
  title = {The Invention of Order},
  publisher = {Duke University Press},
  year = {2025},
  doi = {10.1353/book.142307},
  note = {
  CORE ARGUMENT: Retraces the colonial origins of spatial organization in the Americas and Caribbean and its impact on modern structures of knowledge, power, race, gender, and understandings of global modernity. The coloniality of space dispossessed Indigenous, African, and mixed populations through new systems of control and movement (urban grids, Atlantic trade routes). Drawing on Dussel, Glissant, and Wynter, shows how movement (who travels, settles, is excluded) becomes essential to colonial control, while novel resistance and insurgency geographies emerge in Caribbean excess.

  RELEVANCE: Book-length study extending Dussel's spatial analysis of modernity's colonial origins. The focus on geography and movement complements Dussel's emphasis on 1492 as the moment when European spatial domination produces modernity's center/periphery structure. Connects Dussel to Caribbean decolonial thought.

  POSITION: Constructive extension integrating Dussel with Caribbean philosophy.
  },
  keywords = {spatial-colonial, caribbean, resistance-geographies, Low},
  language = {english},
  abstract = {In The Invention of Order, Don Thomas Deere retraces the colonial origins of spatial organization in the Americas and the Caribbean and its lasting impact on modern structures of knowledge, power, race, gender as well as understandings of global modernity. The coloniality of space dispossessed Indigenous, African, and mixed populations as it constructed new systems of control and movement. Deere demonstrates how these developments manifested, among other forms, in urban grid patterns imposed during the development of Spanish colonial cities as well as totalizing trade routes crisscrossing the Atlantic. Drawing on a range of thinkers including Enrique Dussel, douard Glissant, and Sylvia Wynter, Deere reveals how movementwho travels, who settles, and who is excludedbecomes an essential component of control under colonial rule. Against the violence of spatial reordering, Deere outlines how novel forms of resistance and insurgency geographies still take hold, particularly in the Caribbean, where landscapes remain excessive, eruptive, and uncaptured by the order of modernity.},
  abstract_source = {s2},
}

@book{Dussel2013,
  author = {Dussel, Enrique},
  title = {Ethics of Liberation in the Age of Globalization and Exclusion},
  year = {2013},
  publisher = {Duke University Press},
  address = {Durham},
  editor = {Vallega, Alejandro A.},
  translator = {Mendieta, Eduardo and Bustillo, Camilo Pérez and Angulo, Yolanda and Maldonado-Torres, Nelson},
  doi = {10.1515/9780822395218},
  language = {english},
  note = {
  CORE ARGUMENT: Dussel develops a three-tiered ethics of liberation combining material (life-reproduction), formal (validity through discourse), and feasibility (strategic-institutional) criteria. He argues that dominant ethical systems presuppose the exclusion of victims and that ethics must begin from the interpellation of those denied life and recognition. The material principle—obligation to produce, reproduce, and develop human life—provides a universal but non-Eurocentric foundation grounded in corporeal vulnerability rather than abstract reason.

  RELEVANCE: This is the mature statement of Dussel's ethics after his decade-long debate with Karl-Otto Apel. It directly addresses the charge of Eurocentrism against Kantian and discourse ethics by showing how they presuppose an ideal community of communication that excludes victims of globalization. The concept of "exteriority" (those outside the system) provides Dussel's key diagnostic tool for identifying Eurocentrism in ethical theory—not merely as bias but as structural exclusion.

  POSITION: Defends a material ethics that grounds normativity in the appeals of victims while incorporating discourse-ethical procedures. Argues that material ethics has priority over formal ethics because discourse presupposes participants whose life has already been affirmed.
  },
  keywords = {ethics-liberation, material-ethics, exteriority, discourse-ethics, High, INCOMPLETE, no-abstract}
}

@book{AllenMendieta2021,
  editor = {Allen, Amy and Mendieta, Eduardo},
  title = {Decolonizing Ethics: The Critical Theory of Enrique Dussel},
  year = {2021},
  publisher = {Penn State University Press},
  address = {University Park, Pennsylvania},
  doi = {10.1515/9780271090320},
  language = {english},
  note = {
  CORE ARGUMENT: This collection assesses Dussel's ethics of liberation from multiple critical perspectives, including essays on his debate with Apel over material vs. formal ethics, his engagement with Frankfurt School critical theory, and his concept of transmodernity. Contributors examine whether Dussel's ethics successfully escapes Eurocentrism or replicates modernist assumptions under new terms. The volume includes Dussel's own essay on multiple modernities and an autobiographical reflection on his philosophical trajectory.

  RELEVANCE: Provides critical engagement with Dussel's ethics from sympathetic but probing scholars. Jorge Zúñiga's chapter on grounding the material principle of life addresses the central question of whether Dussel's ethics is relativist or genuinely universal. Amy Allen's essay on ethics and progress examines whether Dussel's critique of Eurocentrism inadvertently relies on Eurocentric categories of development. Essential for understanding contemporary debates over Dussel's ethics.

  POSITION: Critical assessment of liberation ethics from multiple angles, with particular attention to the relationship between Dussel's ethics and Frankfurt School critical theory.
  },
  keywords = {ethics-liberation, Dussel-Apel-debate, critical-theory, decolonial, High},
  abstract = {Enrique Dussel is Latin America’s foremost philosopher, renowned for his contributions to ethics, political philosophy, and liberation theology. Designed for classroom use, this collection of essays engages with Dussel’s encyclopedic work, making his valuable contributions accessible to English-speaking students. In addition to being one of the most original, prolific, and widely known members of the Latin American Philosophy of Liberation movement, Dussel has also made important contributions to world philosophy, the history of philosophy, the history of the Catholic Church in Latin America, and the understanding of Karl Marx. Dussel famously engaged in a decade-long debate with Karl-Otto Apel on the relationship between material and formal ethics—that is, between an ethics of the community of life and an ethics of the community of discourse—and he has produced novel interpretations and analyses of the concepts of alterity, exteriority, the other, and the world history of ethical systems. Most recently, Dussel extended his work on an ethics of liberation into a politics of liberation, developed over the course of three published volumes. In this book, scholars from around the world assess Dussel’s work in ways that are both appreciative and critical. Two essays by Dussel bookend the volume: the collection opens with a consideration of the (im)possibility of multiple modernities and ends with an autobiographical trajectory of the philosopher’s thinking. In addition to Dussel and the editors, the contributors to this volume include Linda Martín Alcoff, Don Thomas Deere, Oscar Guardiola-Rivera, Mario Sáenz Rovner, Alejandro A. Vallega, and Jorge Zúñiga M.},
  abstract_source = {openalex},
}

@book{Mills2018,
