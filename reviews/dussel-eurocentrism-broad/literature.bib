  keywords = {decolonial-turn, exteriority, coloniality, ethics-liberation, High, INCOMPLETE, no-abstract}
}

@article{Dussel1997,
  author = {Dussel, Enrique},
  title = {The Architectonic of the Ethics of Liberation: On Material Ethics and Formal Moralities},
  journal = {Philosophy and Social Criticism},
  year = {1997},
  volume = {23},
  number = {3},
  pages = {1--35},
  language = {english},
  note = {
  CORE ARGUMENT: Dussel articulates the relationship between material ethics (grounded in life-reproduction) and formal moralities (grounded in discourse and validity). He argues that Kantian and discourse ethics are formal but must presuppose material conditions—participants must be alive, fed, capable of communication. Material ethics thus has priority: it establishes the conditions for any formal ethics. Dussel concludes with six principles for formulating a material ethics that is universalizable yet addresses massive poverty and exclusion.

  RELEVANCE: This article presents Dussel's core argument in the debate with Apel. It directly addresses how discourse ethics is Eurocentric by showing that it presupposes an ideal communication community from which victims are excluded. The material principle provides an evaluative standpoint available to those outside the system—victims know that the system denies life. This is Dussel's key move for grounding ethical critique in exteriority.

  POSITION: Defends the priority of material ethics over formal ethics while incorporating procedural validity.
  },
  keywords = {material-ethics, discourse-ethics, Dussel-Apel-debate, High, INCOMPLETE, no-abstract}
}

@incollection{Apel2000,
  author = {Apel, Karl-Otto},
