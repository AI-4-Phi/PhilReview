  },
  keywords = {Dussel-Apel-debate, discourse-ethics, philosophy-of-liberation, High, INCOMPLETE, no-abstract}
}

@article{Demenchonok2025intercultural,
  author = {Demenchonok, Edward},
  title = {Latin American Philosophy of Liberation and Intercultural Philosophy of Dialogue},
  journal = {RUDN Journal of Philosophy},
  year = {2025},
  volume = {29},
  number = {3},
  pages = {795--818},
  doi = {10.22363/2313-2302-2025-29-3-795-818},
  abstract = {The research examines the Latin American philosophy of liberation (L. Cea, A. Roig, E. Dussel), from which the intercultural philosophy of dialogue was developed. The dialogue between Russian and Latin American philosophers and the recognition of the philosophy of liberation in Russia are shown. Attention is paid to the discussion about the dialectic of the universal and the culturally-specific, using the example of Latin American philosophy. The article examines intercultural philosophy, which justifies the right to cultural diversity and the normativity of dialogical relations in a culturally diverse world. It analyzes R. Fornet-Betancourt's project on the intercultural transformation of philosophy, which is based on the cultural embeddedness of philosophical traditions.},
  note = {
  CORE ARGUMENT: Examines the development of intercultural philosophy of dialogue from Latin American liberation philosophy, analyzing how Dussel and Fornet-Betancourt navigate between abstract universalism and relativism through dialogical intercultural relations. Critiques abstract universalism while proposing contextualized concrete universality.

  RELEVANCE: Recent (2025) analysis connecting Dussel's liberation philosophy to intercultural philosophy, addressing the central Domain 5 question of how intercultural dialogue grounds universal norms without imposing Eurocentric categories. Provides contemporary perspective on the universalism-particularity dialectic in Latin American thought.

  POSITION: Represents current scholarship positioning intercultural philosophy as response to both hegemonic universalism and fragmented relativism, emphasizing dialogue as the method for achieving concrete universality.
  },
  keywords = {intercultural-philosophy, dialogue, concrete-universality, High}
}

@article{Acosta2019interculturalidad,
